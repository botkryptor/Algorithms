#include<stdio.h>

void search(vector<int> arr)
{
  int lo=0;
  int hi=arr.size()-1;
  
  while(lo<hi)
  {
    int mid=lo+(hi-lo)/2;
    if(mid%2==0)
    {
      if(arr[mid]==arr[mid+1]
      {
        lo=mid+2;
      }
      else 
      hi=mid;
      
    }
    
    else 
    {
      if(arr[mid]==arr[mid-1]
      {
        lo=mid+1;
      }
      
      else
      hi=mid-1;
      
    }
    
    return arr[lo];
    
    
  
  }
  





}
